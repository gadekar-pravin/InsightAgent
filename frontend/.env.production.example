# InsightAgent Production Environment
# Copy this file to .env.production and fill in values
# Backend API is proxied via Firebase Hosting rewrites (see firebase.json)

# API Key for authentication (get from demo-on.sh or Cloud Run env vars)
VITE_API_KEY=your-api-key-here

# API Base URL - empty for relative paths (proxied via Firebase Hosting)
VITE_API_BASE_URL=
