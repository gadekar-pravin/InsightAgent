# ===========================================
# InsightAgent Environment Configuration
# ===========================================
# Copy this file to .env and fill in your values
# IMPORTANT: Never commit .env to version control

# GCP Project Configuration
GCP_PROJECT_ID=your-project-id
VERTEX_LOCATION=asia-south1

# BigQuery Configuration
BQ_DATASET_ID=insightagent_data

# Firestore Configuration
FIRESTORE_COLLECTION_PREFIX=insightagent

# Vertex AI Configuration
GEMINI_MODEL=gemini-2.5-flash
RAG_CORPUS_NAME=projects/{project}/locations/{location}/ragCorpora/{corpus_id}

# API Configuration
API_HOST=0.0.0.0
API_PORT=8080

# Security (Demo)
# Generate with: python -c "import secrets; print(secrets.token_urlsafe(32))"
DEMO_API_KEY=your-demo-api-key-here
ALLOWED_CORS_ORIGIN=http://localhost:5173

# Environment
ENVIRONMENT=development
